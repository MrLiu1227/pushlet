<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.css.app.base.attachconfig.model.Attachconfig" table="attachconfig">

		<id name="uuid" type="java.lang.String">
			<column name="uuid" length="50" />
			<generator class="assigned" />
		</id>
		<property name="name" type="java.lang.String">
			<column name="name" length="50" />
		</property>

		<property name="orderNum" column="orderNum" type="java.lang.Integer" />

		<property name="tableName" type="java.lang.String">
			<column name="table_name" length="50" />
		</property>
		<property name="tableKey" type="java.lang.String">
			<column name="table_key" length="50" />
		</property>
		<property name="fileLength" type="java.lang.Integer">
			<column name="file_length" length="10" />
		</property>
		<property name="fileNumber" type="java.lang.Integer">
			<column name="file_number" length="10" />
		</property>
		<property name="fileExt" type="java.lang.String">
			<column name="file_ext" length="255" />
		</property>
		<property name="extraPara" type="java.lang.String">
			<column name="extra_para" length="255" />
		</property>
	</class>
</hibernate-mapping>
